{
  "name": "templat",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "relay": "relay-compiler",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "schema": "gq http://localhost:3280/graphql --introspect > ./app/schema.graphql",
    "migrate": "prisma migrate dev",
    "migrate-prod": "prisma migrate deploy",
    "introspect": "cd hasura && ddn connector introspect my_pg",
    "relationship-add": "cd hasura && ddn relationship add my_pg",
    "model-add": "cd hasura && ddn model add my_pg",
    "command-add": "cd hasura && ddn command add my_pg",
    "init-hasura": "cd hasura && ddn project init",
    "build-hasura": "cd hasura && ddn supergraph build local",
    "build-hasura-prod": "cd hasura && ddn supergraph build create",
    "apply-hasura-prod": "cd hasura && ddn supergraph build apply",
    "start-hasura": "cd hasura && ddn run docker-start",
    "show-hasura": "cd hasura && ddn connector show-resources my_pg"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "babel-plugin-relay": "^18.2.0",
    "expo": "~52.0.40",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.14",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.19",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "graphql": "^16.10.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "react-relay": "^18.2.0",
    "relay-compiler": "^18.2.0",
    "relay-runtime": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-relay": "^18.2.0",
    "@types/react-test-renderer": "^18.3.0",
    "graphqurl": "^2.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "prisma": "^6.5.0",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
